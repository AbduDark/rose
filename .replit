modules = ["php-8.2", "web", "nodejs-20"]
run = "php artisan serve"

[nix]
channel = "stable-25_05"

[deployment]
run = ["sh", "-c", "php artisan serve"]

[[ports]]
localPort = 5173
externalPort = 80
exposeLocalhost = true

[[ports]]
localPort = 8000
externalPort = 8000

[workflows]
runButton = "Laravel Development Setup"

[[workflows.workflow]]
name = "Laravel Development"
author = 45724641
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "php artisan migrate --graceful"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "php artisan db:seed"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "php artisan serve --host=0.0.0.0 --port=8000"

[[workflows.workflow]]
name = "Laravel App"
author = 45724641
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "php artisan key:generate --force"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "php artisan migrate:fresh --seed --force"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "php artisan serve --host=0.0.0.0 --port=8000"

